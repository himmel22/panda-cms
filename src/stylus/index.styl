@import vars

#slides
    .container
        margin-right: auto
        margin-left: auto
        width: 1200px
    min-width: 1200px
    background-color: #111
    height: 366px
    overflow: hidden
    .carousel
        margin-bottom: 0px
        width: 1200px
    .carousel-control
        display: none
    .carousel-indicators
        top: 340px
        right: 120px
        li
            margin-left: 15px
            cursor: pointer
    .item
        img
            margin: auto

#highlights
    min-width: 1200px
    padding-top: 10px
    padding-bottom: 20px
    background-color: #F6F5F3
    .container
        width: 940px
    h3
        color: #79584D
        font-weight: normal
    .content
        position: relative
        h4
            line-height: 18px
            color: #8C523F
            font-size: 14px
            font-weight: bold   
            a
                color: #8C523F
                font-size: 14px
                font-weight: bold
        .time
            position: absolute
            right: 0
            top: 0
            color: #666
        .more
            background-color: #856D54
            color: #FFF
            border-radius: 5px
            padding: 2px 5px
    #contact
        h2
            font-size: 14px
        ul
            padding: 0
            margin: 0
        li
            list-style-type: none



            